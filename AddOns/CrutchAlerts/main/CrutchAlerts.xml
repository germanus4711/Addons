            <Controls>
                <Backdrop name="$(parent)Backdrop" centerColor="75000000" edgeColor="88000000" hidden="true">
                    <AnchorFill/>
                    <Edge edgeSize="1"/>
                </Backdrop>
                <Label name="$(parent)Label" font="ZoFontWinH1" color="FFFFFF" text="Boss in |cffee003.0|r" verticalAlignment="CENTER" horizontalAlignment="CENTER" hidden="true">
                    <Dimensions x="600" y="36"/>
                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                </Label>
            </Controls>
        </TopLevelControl>

